{
    "name": "vannut/laravel-dashboard-ploi-tile",
    "description": "Add a simple overview of selected servers to your dashboard",
    "keywords": [
        ":Van Nut",
        "larave;-dashboard-ploi-tile",
        "vannut"
    ],
    "homepage": "https://github.com/vannut/laravel-dashboard-ploi-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Verbruggen",
            "email": "support@vannut.nl",
            "homepage": "https://vannut.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^7.4",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Vannut\\PloiTile\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vannut\\PloiTile\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vannut\\PloiTile\\PloiTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true


}
